@using System.Web.Mvc
@using DNA.Web.ServiceModel
@helper ShareLinks(string linkback, string title)
{
    <p>
        <a href="http://www.facebook.com/sharer.php?u=@linkback&amp;t=@title" title="Share on Facebook" target="_blank">
            <img alt="Share on Facebook" src="data:image/png;base64," /></a>
        <a href="http://twitter.com/home/?status=@title%3a+@linkback" title="Share on Twitter" target="_blank">
            <img alt="Share on Twitter" src="data:image/png;base64," /></a>
        <a href="https://plus.google.com/share?url=@linkback" title="Share on Google+" target="_blank">
            <img alt="Share on Google+" src="data:image/png;base64," /></a>
        @*    <a href="" title="" target="_blank"><img alt="" src="data:image/png;base64," /></a>*@
        <a href="http://www.tumblr.com/share/link?url=@linkback&amp;name=@title" title="Share on Tumblr" target="_blank">
            <img alt="Share on Tumblr" src="data:image/png;base64," /></a>
        <a href="http://delicious.com/save?v=5&amp;noui&amp;jump=close&amp;url=@linkback&amp;title=@title" title="Add to del.icio.us" target="_blank">
            <img alt="Add to del.icio.us" src="data:image/png;base64," /></a>
        <a href="http://www.stumbleupon.com/submit?url=@linkback&amp;title=@title" title="Stumble it!" target="_blank">
            <img alt="Stumble it!" src="data:image/png;base64," /></a>
        <a href="http://digg.com/submit?url=@linkback&amp;title=@title" title="Digg!" target="_blank">
            <img alt="Digg!" src="data:image/png;base64," /></a>
        <a href="javascript:void(0);" title="Subscribe" target="_blank" data-role="subscriber" data-app="@Request.ApplicationPath">
            <img alt="Subscribe" src="data:image/png;base64," /></a>
    </p>
}
@helper FollowButton(int listID, System.Web.Mvc.UrlHelper Url)
{
    var currentWeb = AppModel.Get().CurrentWeb;
    var list = currentWeb.Lists[listID];
    if (!Request.IsAuthenticated)
    {
    <a href="#logindialog" rel="dialog">@Resources.Social.Follow</a>
    }
    else
    {
        if (!list.Owner.Equals(User.Identity.Name))
        {
    <a data-role="follow" 
    data-id="@listID" 
    data-user="@User.Identity.Name"
    data-isfollowing="@list.IsFollowing(User.Identity.Name)"
    data-follow-url="@Url.Content("~/api/"+currentWeb.Name+"/contents/follow")"
    data-unfollow-url="@Url.Content("~/api/"+currentWeb.Name+"/contents/unfollow")"
    data-following-url="@Url.Content("~/api/"+currentWeb.Name+"/contents/isfollowing")"
    data-follow-text="@Resources.Social.Follow"
    data-unfollow-text="@Resources.Social.Unfollow"
    >@Resources.Social.Follow</a>
        }
    }
}
@helper FollowButton(string userName, UrlHelper Url)
{
}
@helper Follwers(int listID)
{
    var list = AppModel.Get().CurrentWeb.Lists[listID];
    @UserAvatarList(list.Followers) 
}
@helper Follwers(string userName, int returns = 0)
{
    var query = AppModel.Get().Users[userName].Followers;
    var results = returns > 0 ? query.Take(returns).ToList() : query.ToList();
    @UserAvatarList(results) 
}
@helper Follows(string userName, int returns = 0)
{
    var query = AppModel.Get().Users[userName].Follows;
    var results = returns > 0 ? query.Take(returns).ToList() : query.ToList();
    @UserAvatarList(results) 
}
@helper UserAvatarList(IEnumerable<DNA.Web.UserProfile> users)
{
    if (users != null)
    {
    <ul data-role="listview" data-inline="true">
        @foreach (var u in users)
        {
            <li title="@u.DisplayName">
                <img src="@(string.IsNullOrEmpty(u.Avatar) ? DNA.Utility.UrlUtility.CreateUrlHelper().Content("~/content/images/no-avatar.gif") : u.Avatar)?w=48&h=48" style="max-width:48px;max-height:48px;"/>
            </li>
        }
    </ul>
    }
}
@helper ResharePeopels(ContentDataItemDecorator item)
{
    var list = item.Parent;
    if (list.AllowResharing)
    { 
    @UserAvatarList(item.GetResharePeoples());
    }
}

@helper UserAvatar(string userName, int size = 64,string style="")
{
    var Url = DNA.Utility.UrlUtility.CreateUrlHelper();
    var defaultAvatar = "~/content/images/no-avatar.gif";
    var user = AppModel.Get().Users[userName];
    if (user != null)
    {
        var avatar = !string.IsNullOrEmpty(user.DefaultProfile.Avatar) ? user.DefaultProfile.Avatar : defaultAvatar;
    <a href="~/social/user/@userName" 
        style="width:@(size)px;height:@(size)px;text-align:center;@style" 
        class="d-avatar d-inline" 
        title="@user.DisplayName">
        <img src="@Url.Content(avatar)" style="max-height:@(size)px;max-width:@(size)px;"/>
    </a>
    }
}

@helper UserInfo(string userName)
{
    var user = AppModel.Get().Users[userName];
    <div style="border: 1px solid #efefef;">
        @UserAvatar(userName, 100)
        <div style="float: left; padding: 0px 10px;">
            <h4 style="margin: 0px; margin-bottom: 10px; font-size: 16pt;">@user.DefaultProfile.DisplayName</h4>
            <div><span style="width: 50px; display: inline-block;">@Resources.Social.Follows:</span><span style="width: 100px; display: inline-block;">@user.Follows.Count()</span><span style="padding-left: 10px; width: 80px; display: inline-block;">@Resources.Social.Followers:</span><span style="width: 100px; display: inline-block;">@user.Followers.Count()</span></div>
            <div><span style="width: 50px; display: inline-block;">@Resources.Social.Posts:</span><span style="width: 100px; display: inline-block;">0</span><span style="padding-left: 10px; width: 80px; display: inline-block;">@Resources.Social.Reposts:</span><span style="width: 100px; display: inline-block;">0</span></div>
        </div>
        <div style="float: left; padding-top: 30px;">
            <a data-role="button">+ @Resources.Social.Follow</a>
        </div>
        <div style="clear: left;"></div>
    </div>
}

@helper Comments(string idPrefix, string url = "", bool isReadOnly = false)
{
    var Url = DNA.Utility.UrlUtility.CreateUrlHelper();
    var srcID = idPrefix + "_cmmSrc";
    var tmplID = idPrefix + "_cmmTmpl";
    if (string.IsNullOrEmpty(url))
    {
        url = Request.Url.ToString();
    }
    if (!isReadOnly)
    {
    <div style="margin-bottom: 10px;position:relative;">
        @if (Request.IsAuthenticated)
        {
            @UserAvatar(User.Identity.Name, 48,"position:absolute;left:10px;top:0px;")
        }
        else
        {
            <a href="javascript:void(0);" 
                title="@Resources.Commons.Login"
                onclick="$.login('@Resources.Commons.Login').done(function(){ location.reload(); });"
                style="position:absolute;left:10px;top:0px;">
                <img src="~/content/images/no-avatar.gif" class="d-inline" />
            </a>
        }
        <div style="padding-left: 64px;">
            <textarea data-role="commentbox"
                data-source="@srcID"
                placeholder="@Resources.Social.CommentHolder"
                data-text-post="@Resources.Commons.Post"
                style="width:97%;">
            </textarea>
        </div>
    </div>
    }
    <ul data-role="listview"
        data-source="@srcID"
        data-tmpl="@tmplID"
        data-scroller="win"
        class="d-comments">
    </ul>
    <script type="text/x-jquery-tmpl" id="@tmplID">
        <a class="avatar" href="${actor.url}">
            <img src="${actor.image.url}" alt="${actor.dispName}" />
        </a>
        <div class="detail d-box1" id="comm_${id}">
            <div class="names">
                <a class=" d-link d-inline dispName" href="${actor.url}">${actor.displayName}</a><a class="d-inline userName" href="${actor.url}">@@${actor.id}</a>
                <span class="posted">{{= $.friendlyDate(published,{
                     theDayBeforeYesterday: "@Resources.Commons.TheDayBeforeYesterday.ToString().ToLower()",
                     yesterday: "@Resources.Commons.Yesterday.ToString().ToLower()",
                     ago: " @Resources.Commons.Ago.ToString().ToLower()",
                     days: "@Resources.Commons.Days.ToString().ToLower()",
                     hours: "@Resources.Commons.Hours.ToString().ToLower()",
                     minutes: "@Resources.Commons.Minutes.ToString().ToLower()",
                     justNow:"@Resources.Commons.Justnow.ToString().ToLower()"
                })}}</span>
            </div>
            <div class="content">${object.content}</div>
            <div class="tools">
                <a role="link" data-icon-left="d-icon-bubbles-2" style="text-transform: lowercase"
                    onclick="var self=$(this);if (!$('#replies_holder_${id}').dataBool('loaded')) { var loader=self.children().first(); loader.removeClass('d-icon-bubbles-2').addClass('d-icon-loading'); var holder= $('#replies_holder_${id}'); $('#replies_${id}').taoDataSource('read').done(function(){ holder.data('loaded',true); loader.addClass('d-icon-bubbles-2').removeClass('d-icon-loading');holder.slideDown().children('.d-comment-box').children('textarea').focus(); }); } else { holder.slideToggle(); }">
                   @Resources.Social.Replies (${replies.totalItems})</a>
                {{if $.isAuth() && actor.id==$.currentUser().id }}
                <a role="link" data-icon-left="d-icon-cross-3" style="text-transform: lowercase"
                    onclick="$.confirm('@(string.Format(Resources.Messages.ConfirmDelete_Format,Resources.Social.Comment))','@Resources.Commons.Confirm').done(function(){$.loading(); $.post('@Url.Content("~/api/comments/delete")',{ id:${id} },function(){ $.loading('hide'); $('#comm_${id}').closest('li').remove(); });});">@Resources.Commons.Delete</a>
                {{/if}}
                {{if $.isAuth() && actor.id!=$.currentUser().id}}
                <a role="link" data-icon-left="d-icon-flag" style="text-transform: lowercase" onclick="$.reportAbuse('${link}', '${actor.id}', '${object.objectType}','${object.content}','@Resources.Commons.ReportAbuse');">@Resources.Commons.Abuse</a>
                {{/if}}
            </div>
            <div data-loaded="false" id="replies_holder_${id}" class="replies">
                @if (!isReadOnly)
                {
                    <div style="position:relative; margin-bottom: 10px;">
                        @if (Request.IsAuthenticated)
                        {
                            @UserAvatar(User.Identity.Name, 48,"position:absolute;left:10px;top:0px;")
                        }
                        else
                        {
                            <a href="javascript:void(0);"
                                onclick="$.login('@Resources.Commons.Login').done(function(){ location.reload(); });"
                                class="avatar"
                                style="position:absolute;left:10px;top:0px;"
                                title="@Resources.Commons.Login">
                                <img src="~/content/images/no-avatar.gif" />
                            </a>
                        }
                        <div style="margin-left: 10px;padding-left:64px;">
                            <textarea data-role="commentbox"
                                data-source="replies_${id}"
                                style="width:97%;"
                                placeholder="@Resources.Social.CommentHolder"
                                data-text-post="@Resources.Commons.Post"></textarea>
                        </div>
                    </div>
                }
                <div data-role="datasource" id="replies_${id}" data-server-paging="true">
                    <div data-action="read" data-url="@(Url.Content("~/api/comments?url=" + url))">
                        <div data-name="replyTo" data-value="${id}"></div>
                    </div>
                    @if (!isReadOnly)
                    {
                        <div data-action="insert" data-method="POST" data-url="@(Url.Content("~/api/comments/add"))">
                            <div data-name="url" data-value="@url"></div>
                            <div data-name="replyTo" data-value="${id}"></div>
                        </div>
                    }
                </div>
                <ul data-role="listview"
                    data-source="replies_${id}"
                    data-tmpl="@tmplID"
                    data-scroller="win"
                    data-autobind="false"
                    class="d-comments">
                </ul>
            </div>
        </div>
    </script>
    <div data-role="datasource" data-server-paging="true" id="@srcID">
        <div data-action="read" data-url="@(Url.Content("~/api/comments?url=" + url))">
            <div data-name="replyTo" data-value="0"></div>
        </div>
        @if (!isReadOnly)
        {
            <div data-action="insert" data-method="POST" data-url="@(Url.Content("~/api/comments/add"))">
                <div data-name="url" data-value="@url"></div>
            </div>
        }
    </div>
}